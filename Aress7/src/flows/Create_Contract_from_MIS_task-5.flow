        </rules>
    </decisions>
    <formulas>
        <name>frContractTerm</name>
        <dataType>Number</dataType>
        <expression>({!countMonthOne} / {!socketsOneMonth})* 1 + 
({!countOneYear} / {!socketsOneYear}) * 12 + 
({!countThreeYear} / {!socketsThreeYear}) *36</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>frCountSocketsOneYear</name>
        <dataType>Number</dataType>
        <expression>{!socketsOneYear} + {!singleOppLineItem.Sockets__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>frCountSocketsThreeYear</name>
        <dataType>Number</dataType>
        <expression>{!socketsThreeYear} +{!singleOppLineItem.Sockets__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>frSocketNumbersOneMonth</name>
        <dataType>Number</dataType>
        <expression>{!socketsOneMonth} + {!singleOppLineItem.Sockets__c}</expression>
        <scale>2</scale>
    </formulas>
