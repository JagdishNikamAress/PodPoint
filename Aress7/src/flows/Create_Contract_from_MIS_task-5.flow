    <decisions>
        <name>DataMIS</name>
        <label>Data &amp; MIS</label>
        <locationX>339</locationX>
        <locationY>234</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Data_MIS_1_month</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>singleOppLineItem.Product_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Data &amp; MIS (1 month)</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>X1_month</targetReference>
            </connector>
            <label>Data &amp; MIS (1 month)</label>
        </rules>
        <rules>
            <name>Data_MIS_1_year</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>singleOppLineItem.Product_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Data &amp; MIS (1 year)</stringValue>
                </rightValue>
            </conditions>
