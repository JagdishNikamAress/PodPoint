            </connector>
            <label>Data &amp; MIS (1 year)</label>
        </rules>
        <rules>
            <name>Data_MIS_3_year</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>singleOppLineItem.Product_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Data &amp; MIS (3 year)</stringValue>
