            <label>Case Or Order</label>
        </rules>
    </decisions>
    <formulas>
        <name>frContactTerm</name>
        <dataType>Number</dataType>
        <expression>({!countMonthOne} * 1 + {!countOneYear} *12 + {!countThreeYear} *36)
*
(({!countMonthOne} + {!countOneYear} + {!countThreeYear}) / {!TotalSockets})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>frTotalSockets</name>
        <dataType>Number</dataType>
        <expression>{!TotalSockets} + {!singleOppLineItem.Sockets__c}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>strContractName</name>
        <dataType>String</dataType>
        <expression>if(
OR (
{!strSource} = &apos;Case&apos; ,
{!strSource} = &apos;Order&apos; ),
&apos;Warranty Contract -&apos; + {!strOppName},&apos;MIS Contract -&apos; + {!strOppName}
)</expression>
    </formulas>
    <formulas>
        <name>Today</name>
        <dataType>Date</dataType>
        <expression>Today()</expression>
    </formulas>
