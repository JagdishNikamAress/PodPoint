<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignments>
        <name>Set_Flag_to_true</name>
        <label>Set Flag to true</label>
        <locationX>783</locationX>
        <locationY>381</locationY>
        <assignmentItems>
            <assignToReference>ContainsSpecificProduct</assignToReference>
            <operator>Assign</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Loop_all_opp_line_items</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Contains_MIS_products</name>
        <label>Contains MIS products</label>
        <locationX>1339</locationX>
        <locationY>63</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Contain_MIS_products</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ContainsSpecificProduct</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Task</targetReference>
            </connector>
            <label>Contain MIS products</label>
        </rules>
    </decisions>
    <decisions>
        <name>Specific_Product</name>
        <label>Specific Product</label>
        <locationX>1110</locationX>
        <locationY>378</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Product_is_Data_MIS</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SingleOppLineItems.Product_Name__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Data &amp; MIS</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Flag_to_true</targetReference>
            </connector>
            <label>Product is Data &amp; MIS</label>
        </rules>
    </decisions>
    <description>Update Opp stage to ready to invoice once a managed installation case is changed to Ready to Commissionl This flow will be classed from a process.</description>
    <formulas>
        <name>OpportunityID</name>
        <dataType>String</dataType>
        <expression>LEFT({!OppID},15)</expression>
    </formulas>
    <interviewLabel>Update Opp stage to Ready to Invoice {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Update Opp stage to Ready to Invoice</label>
    <loops>
        <name>Loop_all_opp_line_items</name>
        <label>Loop all opp line items</label>
        <locationX>955</locationX>
        <locationY>152</locationY>
        <assignNextValueToReference>SingleOppLineItems</assignNextValueToReference>
        <collectionReference>ListOppLineItems</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Specific_Product</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Create_Lead</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processType>AutoLaunchedFlow</processType>
    <recordCreates>
