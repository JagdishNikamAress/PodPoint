            </connector>
            <label>Does not Contain Specific Product</label>
        </rules>
    </decisions>
    <decisions>
        <name>If_Status_is_not_closed</name>
        <label>If Status is not closed</label>
        <locationX>1017</locationX>
        <locationY>685</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>If_status_is_not_closed_yet</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>EachSurveyCase.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Approved by Customer</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>EachSurveyCase.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Rejected by Customer</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>EachSurveyCase.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Closed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Change_Status</targetReference>
            </connector>
            <label>If status is not closed yet</label>
        </rules>
    </decisions>
    <decisions>
        <name>Install_Product_Only</name>
        <label>Install Product Only</label>
        <locationX>1327</locationX>
        <locationY>20</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Install_Product</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>QuoteLineItem.Product_Family__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Installs</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>QuoteLineItem.Product_Family__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MIS</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>QuoteLineItem.Product_Family__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Shipping</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>QuoteLineItem.Product_Family__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Data</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>QuoteLineItem.Product_Family__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Warranty</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>If_Contacts_Specific_Product</targetReference>
            </connector>
            <label>Install Product</label>
        </rules>
        <rules>
            <name>Not_Install_Product</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>QuoteLineItem.Product_Family__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Installs</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Intsall_Only_Flag_False</targetReference>
            </connector>
            <label>Not Install Product</label>
        </rules>
    </decisions>
