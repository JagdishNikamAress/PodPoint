            </connector>
            <label>Contains Specific Product</label>
        </rules>
        <rules>
            <name>doesnotContains_Specific_Product</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ContaintsSpecificProduct</leftValueReference>
                <operator>EqualTo</operator>
