    <decisions>
        <name>If_Contacts_Specific_Product</name>
        <label>If Contacts Specific Product</label>
        <locationX>1334</locationX>
        <locationY>232</locationY>
        <defaultConnector>
            <targetReference>Look_all_line_items</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>If_Specific_Product</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ProductName</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>MIS</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>ProductName</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Shipping</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>ProductName</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Data</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>ProductName</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Warranty</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Contacts_Specifc_Product</targetReference>
            </connector>
            <label>If Specific Product</label>
        </rules>
    </decisions>
    <decisions>
        <name>If_Containts_Specific_Product</name>
