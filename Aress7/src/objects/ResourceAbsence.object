    <validationRules>
        <fullName>Overtime_Resource_Absence_validation1</fullName>
        <active>true</active>
        <description>Validation Rule on Resource Absence (only for &quot;Overtime&quot; record type) that &quot;Start Time&quot; field cannot be less than 4pm same day</description>
        <errorConditionFormula>IF(TEXT(Start)&lt;&gt;TEXT(End), AND(ISNEW(), RecordType.DeveloperName =&apos;Overtime&apos;, VALUE( MID( TEXT( Start), 12, 2 ) ) &lt; 16, DATEVALUE(Start-5.5)  &lt;&gt;  DATEVALUE(End)), false)</errorConditionFormula>
        <errorMessage>&quot;Overtime can only be booked After 4 pm.&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Overtime_Resource_Absence_validation2</fullName>
        <active>true</active>
        <description>&quot;End Time&quot; field cannot be more than 8pm on the same day</description>
        <errorConditionFormula>IF(TEXT(Start)&lt;&gt;TEXT(End), AND(ISNEW(), RecordType.DeveloperName =&apos;Overtime&apos;, VALUE( MID( TEXT( End), 12, 2 ) ) &gt; 20, DATEVALUE(Start) &lt;&gt; DATEVALUE(End-5.5)
), false)</errorConditionFormula>
        <errorMessage>End time should not be exceeded than 8p.m.</errorMessage>
