    <fields>
        <fullName>Total_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>if ( TEXT(Opportunity.CurrencyIsoCode) = &quot;GBP&quot;, if( Opportunity.RecordTypeId =&apos;012240000006rJv&apos;, Subtotal , Total_Inc_VAT__c + UK_VAT__c + Workplace_Grant__c ) , Total_Inc_VAT__c + mva__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Total_Inc_VAT__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Opportunity.RecordTypeId =&apos;012240000006rJv&apos;, Opportunity.Amount_Excluding_VAT__c,

IF( Opportunity.Workplace_Grant__c =TRUE, Subtotal ,Opportunity.Amount))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Subtotal</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>UK_VAT__c</fullName>
        <description>Calculates UK VAT</description>
        <externalId>false</externalId>
        <formula>If(Opportunity.RecordTypeId == &apos;012240000006rJv&apos;,  Subtotal -  Total_Inc_VAT__c  , Subtotal/5 )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>UK VAT</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Workplace_Grant__c</fullName>
        <externalId>false</externalId>
        <label>Workplace Grant</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>mva__c</fullName>
        <externalId>false</externalId>
        <formula>Subtotal*0.25</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>mva (25%)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
