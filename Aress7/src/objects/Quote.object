    <validationRules>
        <fullName>Bill_To_or_Ship_To_is_empty</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
OR( ISBLANK(ShippingCity),
 ISBLANK(ShippingStreet ),
ISBLANK(ShippingPostalCode )
  ), 

 OR( ISBLANK(BillingCity),
 ISBLANK(BillingStreet ),
ISBLANK(BillingPostalCode )
 )
)</errorConditionFormula>
        <errorMessage>Bill To and Ship To Address cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Blank_InvoiceORAccounts_Email</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Status),ISPICKVAL(Status, &apos;Accepted&apos;),ISBLANK(Invoice_Accounts_Email__c))</errorConditionFormula>
        <errorDisplayField>Invoice_Accounts_Email__c</errorDisplayField>
        <errorMessage>Please add a Invoice/Accounts Email before accepting the Quote</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quote_cannot_be_created_as_Opp_is_Won</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Opportunity.StageName ) = &apos;Won&apos;</errorConditionFormula>
        <errorMessage>You cannot create or edit any more quote since the opportunity is already Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quote_without_survey</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(TEXT(Survey_Option__c)), ISPICKVAL(Status,&apos;Accepted&apos;))</errorConditionFormula>
        <errorDisplayField>Survey_Option__c</errorDisplayField>
        <errorMessage>Please select a Survey Option before accepting the Quote</errorMessage>
    </validationRules>
